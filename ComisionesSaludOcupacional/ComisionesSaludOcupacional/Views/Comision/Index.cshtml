@model List<ComisionesSaludOcupacional.Models.ViewModels.ComisionTableViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Comisiones</h2>

<div class="row">
    <div class="col-md-12">
        <input type="button" class="btn btn-primary" onclick="document.location.href='@Url.Content("~/Comision/Add")'" value="Crear Comisión"/>
    </div>
</div>
<br>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>Nombre</th>
                <th>Contacto</th>
                <th>Correo</th>
                <th>Teléfono</th>
                <th> </th>
            </tr>

            @foreach(var oObjeto in Model)
            {
            <tr>
                <td>@oObjeto.nombre</td>
                <td>@oObjeto.contacto</td>
                <td>@oObjeto.correo</td>
                <td>@oObjeto.telefono</td>
                <td><input onclick="document.location.href='@Url.Content("~/Representante/Representantes/"+oObjeto.idComision)'" type="button" value="Editar" class="btn btn-default"/></td>
            </tr>
            }
        </table>
    </div>
</div>

