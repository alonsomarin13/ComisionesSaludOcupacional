@model List<ComisionesSaludOcupacional.Models.ViewModels.ComisionTableUserChooser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutEmpty.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">

            @foreach (var oObjeto in Model)
            {
                <tr>
                    <td align="center"><input onclick="document.location.href='@Url.Content("~/ComisionUser/InformacionPrincipal/"+oObjeto.idComision)'" type="button" value="@oObjeto.nombre" class="btn btn-primary" /></td>

                </tr>
            }
        </table>
    </div>
</div>